[user]
    name = Fadi Serapian
    email = fadi.serapian@gmail.com
[core]
    editor = vim
[color]
    ui = true
[alias]
    lg = log --all --graph
    g = git
    st = status
    s = status --short
    br = branch
    co = checkout
    ci = commit
    p = push
[init]
    defaultBranch = main
[interactive]
    diffFilter = diff-so-fancy --patch
[color "diff-highlight"]
    oldNormal = red bold
    oldHighlight = red bold 52
    newNormal = green bold
    newHightlight = green bold 22
[color "diff"]
    meta = 11
    frag = magenta bold
    func = 146 bold
    commit = yello bold
    old = red bold
    new = green bold
    whitespace = red reverse
